{
  "title": "mysql server",
  "description": "mysql",
  "memory_usage": {
    "required": 92,
    "recommended": 96
  },
  "licenses": [
  ],
  "consumers": [
    "ManagedEngine",
    "ManagedService"
  ],
  "stages": [
    {
      "starter": {
        "image": "engines/servicebase",
        "tag": "current"
      },
      "state": {
        "type_path": "cron",
        "service_handle_field": "title",
        "deployment_type": "worker",
        "default_stop_timeout": 0,
        "restart_attempts": 0
      },
      "disposition": {
        "start_syslog": true
      },
      "system_packages": [
        {
          "name": "build-essential"
        },
        {
          "name": "gcc"
        },
        {
          "name": "make"
        },
        {
          "name": "autoconf"
        }
      ],
      "file_permissions": [
        {
          "path": "/home/cron",
          "recursive": false,
          "permissions": "u+w",
          "create": "dir"
        }
      ]
    },
  ]
  }
}
